{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --",
    "docs": "./node_modules/.bin/esdoc && xdg-open ./docs/index.html &"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "^1.9.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^2.3.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.9",
    "prettier": "1.17.1",
    "pretty-quick": "^1.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
